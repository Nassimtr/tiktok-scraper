{"manifest_version": 2, 
"name": "TikTok Scraper", 
"version": "2.9", 
"description": "Scrape TikTok Video URLs", 
"permissions": ["activeTab"],
"browser_action": {"default_popup": "popup.html", "default_icon": {"16": "icon16.png", "48": "icon48.png", "128": "icon128.png"}}, "background": {"scripts": ["background.js"], "persistent": false}, "content_scripts": [{"matches": ["https://www.tiktok.com/*"], "js": ["contentScript.js"]}]}